
Sample Input
3
bat
apple
batman

Sample Output
apple
batman
bat

 Solution in C++

#include<bits/stdc++.h>
using namespace std;
bool comparator(string s1, string s2){

	if(s1.size()<=s2.size() && (s2.substr(0,s1.size())==s1)) return false;
	if(s1.size()>=s2.size() && (s1.substr(0,s2.size())==s2)) return true;
	return s1<s2;
}
int main() {
	int n;
	cin>>n;
	vector<string>vec;
	while(n--)
	{
		string s;
		cin>>s;
		vec.push_back(s);

	}
	sort(vec.begin(),vec.end(),comparator);
	for(auto i:vec)
	cout<<i<<endl;
	return 0;
}
